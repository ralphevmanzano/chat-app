<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:top="-2px" android:right="-2px" android:left="-2px">
    <shape >
      <solid android:color="@android:color/white"/>
      <stroke android:width="1px" android:color="@color/blueGray"/>
    </shape>
  </item>
  <item android:top="-20dp" android:bottom="-20dp">
    <shape >
      <solid android:color="@android:color/transparent"/>
      <stroke android:width="16dp" android:color="@android:color/white"/>
    </shape>
  </item>
</layer-list>
